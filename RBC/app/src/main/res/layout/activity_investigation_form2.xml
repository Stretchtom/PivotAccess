<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.tmutabazi.rbc.InvestigationForm2"
    android:id="@+id/a">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Date of birth"
        android:id="@+id/textView21"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_marginTop="10dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText10"
        android:layout_alignBottom="@+id/textView21"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Gender"
        android:id="@+id/textView10"
        android:layout_below="@+id/editText10"
        android:layout_alignLeft="@+id/textView21"
        android:layout_alignStart="@+id/textView21"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />
    <RadioGroup
        android:id="@+id/radioSex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

    </RadioGroup>

    <RadioButton
        android:id="@+id/radioMale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="M"
        android:checked="true"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignRight="@+id/textView21"
        android:layout_alignEnd="@+id/textView21"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <RadioButton
        android:id="@+id/radioFemale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="F"
        android:checked="false"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_alignTop="@+id/radioMale"
        android:layout_toRightOf="@+id/radioMale"
        android:layout_toEndOf="@+id/radioMale" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nationality"
        android:id="@+id/textView11"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_alignTop="@+id/editText11"
        android:layout_toLeftOf="@+id/editText15"
        android:layout_toStartOf="@+id/editText15" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText11"
        android:layout_alignTop="@+id/radioMale"
        android:layout_toRightOf="@+id/editText12"
        android:layout_toEndOf="@+id/editText12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="ID number"
        android:id="@+id/textView12"
        android:layout_below="@+id/textView10"
        android:layout_toRightOf="@+id/radioSex"
        android:layout_toEndOf="@+id/radioSex"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText12"
        android:layout_alignTop="@+id/textView12"
        android:layout_toRightOf="@+id/radioMale"
        android:layout_toEndOf="@+id/radioMale" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Residential district"
        android:id="@+id/textView13"
        android:layout_below="@+id/editText12"
        android:layout_toRightOf="@+id/radioSex"
        android:layout_toEndOf="@+id/radioSex"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText13"
        android:layout_below="@+id/editText12"
        android:layout_toLeftOf="@+id/radioNo"
        android:layout_toStartOf="@+id/radioNo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/textView14"
        android:layout_below="@+id/editText13"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:layout_marginTop="10dp"
        android:text="Sector "
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText14"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignLeft="@+id/radioMale"
        android:layout_alignStart="@+id/radioMale"
        android:layout_marginTop="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Cell"
        android:id="@+id/textView15"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_below="@+id/editText13"
        android:layout_above="@+id/editText16"
        android:layout_toLeftOf="@+id/editText17"
        android:layout_toStartOf="@+id/editText17"
        android:layout_marginTop="10dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText15"
        android:layout_alignTop="@+id/editText14"
        android:layout_toRightOf="@+id/textView17"
        android:layout_toEndOf="@+id/textView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Village"
        android:id="@+id/textView16"
        android:layout_below="@+id/textView14"
        android:layout_toRightOf="@+id/radioSex"
        android:layout_toEndOf="@+id/radioSex"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText16"
        android:layout_alignBottom="@+id/textView16"
        android:layout_toRightOf="@+id/textView16"
        android:layout_toEndOf="@+id/textView16" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Pregnant?"
        android:id="@+id/textView17"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_above="@+id/textView18"
        android:layout_toRightOf="@+id/editText16"
        android:layout_toEndOf="@+id/editText16" />
    <RadioGroup
        android:id="@+id/radioPregnant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

    </RadioGroup>

    <RadioButton
        android:id="@+id/radioYes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes"
        android:checked="true"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_alignBottom="@+id/editText16"
        android:layout_toLeftOf="@+id/radioNo"
        android:layout_toStartOf="@+id/radioNo" />

    <RadioButton
        android:id="@+id/radioNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No"
        android:textColor="#ff000000"
        android:textSize="30dp"
        android:layout_alignTop="@+id/radioYes"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Head of household name"
        android:id="@+id/textView18"
        android:layout_below="@+id/textView16"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText17"
        android:layout_below="@+id/textView17"
        android:layout_toRightOf="@+id/editText10"
        android:layout_toEndOf="@+id/editText10"
        android:layout_marginTop="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Telephone mobile"
        android:id="@+id/textView19"
        android:layout_below="@+id/editText17"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:textColor="#ff000000"
        android:textSize="30dp" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText18"
        android:layout_below="@+id/textView18"
        android:layout_toRightOf="@+id/editText14"
        android:layout_toEndOf="@+id/editText14"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        android:id="@+id/button2"
        android:textSize="15dp"
        android:textColor="#ff000000"
        android:layout_alignTop="@+id/textView19"
        android:layout_alignRight="@+id/editText17"
        android:layout_alignEnd="@+id/editText17" />

</RelativeLayout>
